                    Delete
                  </button>
                <!-- </a> -->
                </td>
              </tr>
            <%}%>
          </tbody>
          </table>
      </div>

      <div id="menu5" class="tab-pane":class="admindashTab==='transaction' ? 'active' : ''">
        <table class="table table-dark table-hover">
          <thead>
            <tr>
              <th>Username</th>
              <th>TransactionID</th>
              <th>Give power</th>
              <th>Remove</th>  
            </tr>
          </thead>
          <tbody>
            <%for(let i=0;i<locals.transactions.length;i++){%>
              <tr id="mytidtr<%=i%>">
                <td><%=locals.transactions[i][1]%></td>
                <td><%=locals.transactions[i][2]%></td>
                <td>
                  <!-- Button trigger modal -->
                  <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#transactiondrop<%=i%>">
                    Add balance
                  </button>

                  <!-- Modal -->
                  <div class="modal fade" id="transactiondrop<%=i%>" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
                      <div class="modal-content">
                        <div class="modal-header">
                          <h5 class="modal-title" id="staticBackdropLabel">Add to balance</h5>
                        </div>
                        <div class="modal-body" style="background-color:black;color:white">
                          <label for="exampleFormControlInput1" class="form-label"
                              >Amount</label
                            >
                            <input
                              type="text"
                              class="form-control"
                              id="amount<%=i%>"
                              placeholder="Enter amount to be added"
                              name="amount"
                            />
                        </div>
                        <div class="modal-footer">
                          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                          <button onclick="transactionAdd(<%=locals.transactions[i][0]%>,this,'mytidtr<%=i%>',<%=locals.transactions[i][3]%>,'amount<%=i%>')" type="button" class="btn btn-secondary" data-bs-dismiss="modal">Add</button>
                        </div>
                      </div>
                    </div>
                  </div>
                </td>
                <td>
                  <!-- <a href="/submission/delete/<%=locals.transactions[i][0]%>"> -->
                  <button
                    type="button"
                    class="btn btn-danger"
                    data-bs-toggle="modal"
                    onclick="transactionDelete(<%=locals.transactions[i][0]%>,this,'mytidtr<%=i%>')"
                  >
                    Reject
                  </button>
                <!-- </a> -->
                </td>
              </tr>
            <%}%>
          </tbody>
          </table>
      </div>

      <div id="menu6" class="tab-pane":class="admindashTab==='postreport' ? 'active' : ''">
        <table class="table table-dark table-hover">
          <thead>
            <tr>
              <th>Post ID</th>
              <th>Problem list</th>
              <th>Remove post</th>
              <th>Reject</th>  
            </tr>
          </thead>
          <tbody>
            <%for(let i=0;i<locals.reports.length;i++){%>
              <tr id="myreporttr<%=i%>" class="myreporttr<%=locals.reports[i][1]%>">
                <td><%=locals.reports[i][1]%></td>
                <td>
                  <!-- Button trigger modal -->
                  <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#reportdrop<%=i%>">
                    Problems
                  </button>
                  <!-- Modal -->
                  <div class="modal fade" id="reportdrop<%=i%>" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
                      <div class="modal-content">
                        <div class="modal-header">
                          <h5 class="modal-title" id="staticBackdropLabel">Reports of post</h5>
                        </div>
                        <div class="modal-body" style="color:black">
                          <%var token= locals.reports[i][3].split(';')%>
                          <h5>Tags:</h5>
                          <%for(let j=0;j<token.length;j++) {%>
                            <p><%=token[j]%></p>
                          <%}%>
                          <h5>Description:</h5>
                          <pre style="white-space: pre-wrap;"><%=locals.reports[i][4]%></pre>
                        </div>
                        <div class="modal-footer">
                          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                        </div>
                      </div>
                    </div>
                  </div>
                </td>
                <td>
                  <button
                    type="button"
                    class="btn btn-danger"
                    data-bs-toggle="modal"
                    onclick="postDelete(<%=locals.reports[i][1]%>,this,'myreporttr<%=i%>')"
