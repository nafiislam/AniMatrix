                      <!-- Form Group (organization name)-->
                      <div class="col-md-6">
                        <label class="small mb-1" for="inputGender">
                          gender</label
                        >
                        <select id="form3Example6cg" class="form-control form-control-lg" name="country" value="<%= locals.country %>" placeholder="write name of your country">
                          
                            <option value="Male">Male</option>
                            <option value="Female">Female</option>
                            <option value="Others">Others</option>
                          
                        </select>
                      </div>
