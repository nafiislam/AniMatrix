                    <!-- Form Group (username)-->
                    <div class="mb-3">
                      <label class="small mb-1" for="inputUsername"
                        >Username
                      </label>
                      <input
                        class="form-control"
                        id="inputUsername"
                        type="text"
                        placeholder="Enter your username"
                        name="username"
                        value="<%=locals.array[3]%>"
                      />
                    </div>

                    <!-- Form Row        -->
