                      <label class="small mb-1" for="inputEmailAddress"
                        >Email address</label
                      >
                      <input
                        class="form-control"
                        id="inputEmailAddress"
                        type="email"
                        name="email"
                        placeholder="Enter your email address"
                        value="<%=locals.array[1]%>"
                      />
                    </div>
                    <!-- Form Row-->
